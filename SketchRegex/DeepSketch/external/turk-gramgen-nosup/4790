1 ?{repeatatleast(repeatatleast(or(<let>,<vow>),1),1)}
2 ?{repeatatleast(<let>,1),repeatatleast(<vow>,1)}
3 ?{<let>,repeatatleast(<vow>,1)}
4 ?{repeatatleast(repeatatleast(<let>,1),1)}
5 ?{repeatatleast(or(<let>,<vow>),1)}
6 ?{repeatatleast(<vow>,1)}
7 ?{repeatatleast(<let>,1),<vow>}
8 ?{or(<let>,<vow>)}
9 ?{<let>,<vow>}
