1 ?{contain(repeatatleast(or(<let>,<vow>),1))}
2 ?{contain(repeatatleast(<let>,1)),<vow>}
3 ?{contain(or(<let>,<vow>))}
4 ?{repeatatleast(or(<let>,<vow>),1)}
5 ?{contain(<let>),<vow>}
6 ?{repeatatleast(<let>,1),<vow>}
7 ?{or(<let>,<vow>)}
8 ?{contain(repeatatleast(<let>,1))}
9 ?{<let>,<vow>}
10 ?{contain(<let>)}
11 ?{contain(<vow>)}
12 ?{repeatatleast(<let>,1)}
13 ?{<let>}
14 ?{<vow>}
