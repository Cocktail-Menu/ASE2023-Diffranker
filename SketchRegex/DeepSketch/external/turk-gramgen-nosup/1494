1 ?{concat(<low>,repeatatleast(<vow>,1))}
2 ?{repeatatleast(concat(<low>,<vow>),1)}
3 ?{<low>,repeatatleast(<vow>,1)}
4 ?{concat(<low>,<vow>)}
5 ?{concat(<let>,repeatatleast(<vow>,1))}
6 ?{repeatatleast(concat(<let>,<vow>),1)}
7 ?{<low>,<vow>}
