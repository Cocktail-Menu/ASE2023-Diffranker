1 ?{startwith(or(<cap>,<low>))}
2 ?{startwith(<cap>),<low>}
3 ?{startwith(or(<let>,<low>))}
4 ?{startwith(<let>),<low>}
5 ?{or(<cap>,<low>)}
6 ?{startwith(<cap>),<let>}
7 ?{startwith(<low>)}
8 ?{<cap>,<low>}
9 ?{or(<let>,<low>)}
10 ?{startwith(<let>),<let>}
11 ?{startwith(<cap>)}
12 ?{<let>,<low>}
13 ?{startwith(<let>)}
14 ?{<cap>,<let>}
